{
    "manifest_version": 2,
    "name": "Dataset Detector",
    "version": "1.0",
    "description": "Detect datasets in a large dataset using machine learning",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "cloud.png"
    },
    "permissions": ["activeTab"]
  }
